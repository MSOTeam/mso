{
  "name": "mso",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "npm-recursive-install --skip-root",
    "client": "cd client && npm start",
    "server": "cd server && nodemon index.js",
    "start": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\""
  },
  "devDependencies": {},
  "dependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2",
    "recursive-install": "^1.3.0"
  }
}
